#include <stdio.h>

#define MAX 100

int main()
{
	int STACK[MAX];
	int top;
	int f;
	int m, n;

	scanf("%d%d", &m, &n);

	top = 0;
	do {
		if (m*n > 0){
			//(1)
				//(2)
		}else{
			f = m + n + 1;
			if (top > 0){
				//(3)
			}
			//(4)
				n = f;
		}
	}while (!(-1 == top));
	printf("f = %d\n", f);
	return 0;
}
